{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Authentication": "Information"
    }
  },
  "AllowedHosts": "*",
  "KeyVault": {
    "Name": ""
  },
  "Jwt": {
    "Issuer": "https://extratime-api.azurewebsites.net",
    "Audience": "https://extratime.gonek.net",
    "AccessTokenExpirationMinutes": 15,
    "RefreshTokenExpirationDays": 7
  },
  "FootballData": {
    "BaseUrl": "https://api.football-data.org/v4",
    "RequestsPerMinute": 10,
    "SupportedCompetitionIds": [2021, 2014, 2002, 2019, 2015]
  },
  "ExternalData": {
    "Understat": {
      "Enabled": true,
      "SyncHourUtc": 4,
      "SyncSchedule": "0 4 * * *"
    },
    "FootballDataUk": {
      "Enabled": true,
      "SyncHourUtc": 5,
      "SyncSchedule": "0 5 * * 1"
    },
    "ClubElo": {
      "Enabled": true,
      "SyncHourUtc": 3,
      "SyncSchedule": "0 3 * * *"
    },
    "ApiFootball": {
      "Enabled": false,
      "ApiKey": "",
      "EnableEplOnlyInjurySync": true,
      "EnableFplInjuryStatusProvider": true,
      "QuotaPolicy": {
        "HardDailyLimit": 100,
        "OperationalCap": 95,
        "SafetyReserve": 10,
        "MaxInjuryCallsPerDay": 15
      },
      "LineupQualityGate": {
        "EnforceBeforeInjurySync": false,
        "MinLineupCoverageUpcoming24hPercent": 75,
        "MinLineupCoveragePlayedMatchesPercent": 90,
        "MaxLineupParseFailureRatePercent": 5
      },
      "StaleAfterHours": 24
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "https://extratime.gonek.net",
      "https://extratime-web-prod.azurestaticapps.net"
    ]
  }
}
